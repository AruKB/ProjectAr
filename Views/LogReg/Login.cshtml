@model project.Models.Users
@{
    ViewData["Title"]  = "Login";
}
<br/><br/>
<form asp-action="Users_Logins" method="Post">
    <div class="form-horizontal">
        <div class="row">
            <div class="from-group">
            <label asp-for="Users_Logins" class="col-lg-2 col-sm-3 control-label"></label>
            <div class="col-lg-3">
                <input asp-for="Users_Logins" class="form-control"/>
            </div>
        </div>

        <div class="form-group">
            <label asp-for="Users_Password" class="col-lg-2 col-sm-3 control-label"></label>
            <div class="col-lg-3">
                <input type="password" asp-for="Users_Password" class="form-control"/>
            </div>
        </div>
    </div>

    </div>
    <input type="submit" value="Log in" class="col-lg-2 col-md-offset-2 btn btn-success"/><br/>
</form>